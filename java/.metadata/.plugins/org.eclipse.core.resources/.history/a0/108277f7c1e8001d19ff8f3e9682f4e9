package first;

import java.util.Scanner;

public class Gugudan {

	public static void main(String[] args) {
		System.out.println("숫자입력: ");
		Scanner sc = new Scanner(System.in);
		int i = sc.nextInt();
		for (int j = 1; j < 10; j++) {
			System.out.println(i + " * " + j + " = " + i*j);
		}

	}

}
